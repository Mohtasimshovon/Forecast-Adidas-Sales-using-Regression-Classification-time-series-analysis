library(readxl)
library(dplyr)
library(ggplot2)
library(caret)
library(randomForest)
library(cluster)
library(forecast)
library(tseries)
# Load data
df <- read_excel("Adidas US Sales Datasets.xlsx", sheet = "Data Sales Adidas")
View()
# Feature engineering
df$Month <- as.numeric(format(df$`Invoice Date`, "%m"))
df$Year <- as.numeric(format(df$`Invoice Date`, "%Y"))
# ---------------------
# REGRESSION ANALYSIS
# ---------------------
reg_model <- lm(`Total Sales` ~ `Price per Unit` + `Units Sold` + `Operating Margin`, data = df)
summary(reg_model)
# ---------------------
# CLASSIFICATION ANALYSIS
# ---------------------
df$Profit_Category <- cut(df$`Operating Margin`, breaks=c(-Inf, 0.3, 0.5, Inf), labels=c("Low", "Medium", "High"))
set.seed(42)
train_index <- createDataPartition(df$Profit_Category, p=0.8, list=FALSE)
train_data <- df[train_index, ]
test_data <- df[-train_index, ]
rf_model <- randomForest(Profit_Category ~ `Price per Unit` + `Units Sold`, data = train_data)
pred <- predict(rf_model, test_data)
confusionMatrix(pred, test_data$Profit_Category)
# ---------------------
# CLUSTERING ANALYSIS
# ---------------------
scaled_data <- scale(df[, c("Price per Unit", "Units Sold", "Total Sales")])
kmeans_result <- kmeans(scaled_data, centers=3)
df$Cluster <- kmeans_result$cluster
aggregate(df[, c("Total Sales", "Operating Profit")], by=list(df$Cluster), mean)
# ---------------------
# TIME SERIES ANALYSIS
# ---------------------
df_ts <- df %>%
group_by(month = as.Date(cut(`Invoice Date`, "month"))) %>%
summarise(Sales = sum(`Total Sales`))
ts_data <- ts(df_ts$Sales, frequency = 12)
decomp <- decompose(ts_data)
plot(decomp)
